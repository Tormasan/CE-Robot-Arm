<?xml version="1.0"?>
<robot name="robot">
   
    <link name="base_link">
        <visual>
            <geometry>
                <cylinder radius="0.5"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <material name="blue"/>
        </visual>
    </link>
    
    <joint name="J1" type="revolute">
        <parent link="base_link"/>
        <child link="humerus"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit effort="1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
    </joint>
    
    <link name="humerus">
        <visual>
            <geometry>
                <cylinder length="1.9" radius="0.4"/>
